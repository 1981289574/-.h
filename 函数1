//写一个函数，求两个数的最大值

int get_max(int x,int y)
{
	return(x > y ? x : y);
}
int main()
{
	int a = 0;
	int b = 0;
	scanf("%d%d", &a, &b);

	//求两个数的较大值
	int max=get_max(a, b);
	printf("max=%d\n", max);
	return 0;
}
